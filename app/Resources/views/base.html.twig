<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>{% block title %}Блог{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('vendor/bootstrap/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('css/styles.css') }}">
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>

<header class="header">
    <a href="{{ path('homepage') }}"> <img class="logo-icon" src="{{ asset('images/aquanote-logo.png') }}"> </a>
    {% block head %}
        <h1 class="logo">Блог</h1>
    {% endblock %}
    <ul >
        <ul class="navi">
            <li ><a href="{{ path('login') }}">Личный кабинет</a></li>
        </ul>
    </ul>
</header>
{% block body %}{% endblock %}
<div class="footer">
    <p class="footer-text">© Климов С.А.</p>
</div>
{% block javascripts %}
    <script src="//code.jquery.com/jquery-2.1.4.min.js"></script>
    <script src="{{ asset('js/main.js') }}"></script>
{% endblock %}
</body>
</html>